programa {
  real n1
  real n2
  real soma
  real subtracao
  real multiplicacao
  funcao inicio() {
    escreva ("escreva um número: ")
    leia (n1)
    escreva ("\n escreva mais um número: ")
    leia(n2)
    escreva("\n a soma é: ")
    soma = (n1 + n2)
    escreva(soma)
    escreva("\n a subtração é: ")
    subtracao = (n1 - n2)
    escreva(subtracao)
    escreva("\n a multiplicação é: ")
    multiplicacao = (n1 * n2)
    escreva (multiplicacao)
    
    
  }
}
